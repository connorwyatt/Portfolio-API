{
  "name": "portfolio-api",
  "version": "0.0.0",
  "scripts": {
    "prestart": "npm run build",
    "start": "npm run serve:dist",
    "build": "tsc --outDir ./dist/",
    "build:watch": "npm run build -- --watch",
    "format": "clang-format --glob=\"src/**/*.ts\" -i --style=file",
    "serve:dist": "node dist/main.js"
  },
  "devDependencies": {
    "clang-format": "^1.0.45",
    "typescript": "latest"
  },
  "dependencies": {
    "@types/body-parser": "^0.0.33",
    "@types/express": "^4.0.33",
    "@types/node": "^6.0.46",
    "at-express": "^1.0.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "fluency-injection": "^1.1.1",
    "reflect-metadata": "^0.1.8"
  }
}
